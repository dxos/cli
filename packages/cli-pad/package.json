{
  "name": "@dxos/cli-pad",
  "version": "2.12.3",
  "description": "DXOS Pad CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-pad#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "bin": {
    "wire-pad": "bin/dx-pad.js",
    "dx-pad": "bin/dx-pad.js"
  },
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "workspace:*",
    "@dxos/debug": "~2.19.11",
    "assert": "^2.0.0",
    "ipfs-http-client": "48.1.1",
    "js-yaml": "^3.13.1",
    "lodash-clean": "^2.2.1",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "node-yaml": "^4.0.1",
    "semver": "^7.1.1",
    "yargs": "^16.2.0",
    "abort-controller": "~3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.20",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "@types/node": "~16.11.12",
    "@types/yargs": "^16.0.1",
    "@types/jest": "~27.0.3"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c147f853bc07376a163cee05c3c44198387c5655"
}
