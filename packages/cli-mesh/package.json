{
  "name": "@dxos/cli-mesh",
  "version": "2.18.1",
  "description": "DXOS Network CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-mesh#readme",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "license": "AGPL-3.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx-mesh": "bin/dx-mesh.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "build:watch": "tsc --watch",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@dxos/async": "~2.31.3",
    "@dxos/cli-core": "workspace:*",
    "@dxos/network-manager": "~2.31.3",
    "assert": "^2.0.0",
    "boolean": "^3.0.1",
    "do-wrapper": "^4.1.2",
    "performance-now": "^2.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.24",
    "@types/jest": "~27.0.3",
    "@types/node": "~16.11.12",
    "@types/yargs": "^16.0.1",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
