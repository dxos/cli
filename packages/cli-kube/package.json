{
  "name": "@dxos/cli-kube",
  "version": "2.8.6-alpha.0",
  "description": "DXOS KUBE CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-kube#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "wire-kube": "bin/dx-kube.js",
    "dx-kube": "bin/dx-kube.js"
  },
  "files": [
    "bin/",
    "dist/",
    "scripts/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "jest": {
    "preset": "ts-jest/presets/default",
    "testEnvironment": "node",
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@dxos/async": "2.16.3",
    "@dxos/cli-core": "^2.8.6-alpha.0",
    "@dxos/crypto": "2.16.3",
    "@dxos/debug": "2.16.3",
    "@dxos/object-model": "2.16.3",
    "@dxos/registry-client": "2.16.3",
    "assert": "^2.0.0",
    "do-wrapper": "^4.1.2",
    "got": "^11.8.2",
    "js-yaml": "^3.13.1",
    "yamljs": "^0.3.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.10",
    "@types/jest": "^26.0.7",
    "eslint": "^7.13.0",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "uuid": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
