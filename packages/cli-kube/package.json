{
  "name": "@dxos/cli-kube",
  "version": "2.17.0",
  "description": "DXOS KUBE CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-kube#readme",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "license": "AGPL-3.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx-kube": "bin/dx-kube.js"
  },
  "files": [
    "bin/",
    "dist/",
    "scripts/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "build:watch": "tsc --watch",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "preset": "ts-jest/presets/default",
    "testEnvironment": "node",
    "verbose": false
  },
  "dependencies": {
    "@dxos/async": "~2.30.3-dev.c4e7eac9",
    "@dxos/cli-core": "workspace:*",
    "@dxos/crypto": "~2.30.3-dev.c4e7eac9",
    "@dxos/debug": "~2.30.3-dev.c4e7eac9",
    "@dxos/object-model": "~2.30.3-dev.c4e7eac9",
    "@dxos/registry-client": "~2.30.3-dev.c4e7eac9",
    "assert": "^2.0.0",
    "do-wrapper": "^4.1.2",
    "got": "^11.8.2",
    "js-yaml": "^3.13.1",
    "lodash.defaultsdeep": "^4.6.1",
    "node-fetch": "^2.6.1",
    "yamljs": "^0.3.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.24",
    "@types/jest": "~27.0.3",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/node": "~16.11.12",
    "@types/yargs": "^16.0.1",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c147f853bc07376a163cee05c3c44198387c5655"
