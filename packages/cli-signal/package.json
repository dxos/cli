{
  "name": "@dxos/cli-signal",
  "version": "2.12.4",
  "description": "DXOS Signal CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-signal#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "bin": {
    "wire-signal": "bin/dx-signal.js",
    "dx-signal": "bin/dx-signal.js"
  },
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "workspace:*",
    "@dxos/debug": "~2.19.11",
    "lodash-clean": "^2.2.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.20",
    "jest": "^27.4.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "@types/node": "~16.11.12",
    "@types/yargs": "^16.0.1",
    "@types/jest": "~27.0.3",
    "eslint": "^7.13.0"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
