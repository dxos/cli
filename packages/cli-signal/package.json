{
  "name": "@dxos/cli-signal",
  "version": "2.4.15",
  "description": "DXOS Signal CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-signal#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/es/index.js",
  "bin": {
    "wire-signal": "bin/dx-signal.js",
    "dx-signal": "bin/dx-signal.js"
  },
  "files": [
    "bin/",
    "dist/es"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --copy-files",
    "clean": "rm -rf ./dist",
    "cli": "BABEL_CACHE_PATH=./dist/babel-cache.json babel-node ./src/index.js",
    "lint": "semistandard",
    "lint:fix": "semistandard --fix",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests --runInBand --no-cache"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@dxos/cli-core": "^2.4.15",
    "@dxos/debug": "~2.12.22",
    "@wirelineio/registry-client": "^1.1.0-beta.0",
    "lodash-clean": "^2.2.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-import": "^3.0.0",
    "jest": "^27.2.0",
    "semistandard": "14.2.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ],
    "ignore": [
      "/node_modules/",
      "**/node_modules/",
      "**/dist/"
    ]
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
