{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "12"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-inline-import", {
        "extensions": [
          ".yml"
        ]
      }
    ],
    "add-module-exports",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ]
}
