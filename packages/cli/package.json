{
  "name": "@dxos/cli",
  "version": "2.11.3-alpha.0",
  "description": "DXOS CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "bin": {
    "dx": "bin/dx.js",
    "wire": "bin/dx.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/main.ts",
    "dev": "npm run cli",
    "dx": "npm run cli",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "prebuild": "./scripts/get-extensions.sh &> /dev/null",
    "preinstall": "npm run checkNew && npm run checkOld",
    "checkOld": "[ -d ~/.wire ] && [ ! -L ~/.dx ] && ln -s ~/.wire ~/.dx || true",
    "checkNew": "[ ! -d ~/.wire ] && [ ! -d ~/.dx ] && mkdir -p ~/.dx || true"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "testEnvironment": "node",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "^2.11.3-alpha.0",
    "@dxos/debug": "2.19.2",
    "@dxos/random-access-multi-storage": "~2.19.2",
    "assert": "^2.0.0",
    "find-root": "^1.1.0",
    "fs-extra": "9.0.0",
    "got": "^11.8.2",
    "js-yaml": "^3.13.1",
    "lodash.defaults": "^4.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.uniqby": "^4.7.0",
    "ndjson-parse": "^1.0.4",
    "node-yaml": "^4.0.1",
    "ora": "^3.4.0",
    "read-pkg-up": "^6.0.0",
    "semver": "^7.1.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.19",
    "jest": "^27.2.0",
    "json2yaml": "^1.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "yaml2json": "^1.0.2"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c147f853bc07376a163cee05c3c44198387c5655"
}
