    "ora": "^3.4.0",
    "read-pkg-up": "^6.0.0",
    "semver": "^7.1.1",
    "strip-json-comments": "3.1.1",
    "uint8arrays": "1.1.0",
    "url-join": "^4.0.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.24",
    "@types/find-root": "^1.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "~27.0.3",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "~16.11.12",
    "@types/yargs": "^17.0.10",
    "@types/lodash.uniqby": "^4.7.7",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "json2yaml": "^1.1.0",
    "node-jq": "^2.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "yaml2json": "^1.0.2"
