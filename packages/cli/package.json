{
  "name": "@dxos/cli",
  "version": "2.18.3",
  "description": "DXOS CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli#readme",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "license": "AGPL-3.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx": "bin/dx.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "./scripts/get-extensions.sh && tsc && cp -R ./gen/* dx.yml dist",
    "build:watch": "tsc --watch",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/main.ts",
    "dev": "npm run cli",
    "dx": "./bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "testEnvironment": "node",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "workspace:*",
    "@dxos/config": "~2.31.3",
    "@dxos/debug": "~2.31.3",
    "@dxos/random-access-multi-storage": "~2.31.3",
    "assert": "^2.0.0",
    "download": "^8.0.0",
    "find-root": "^1.1.0",
    "fs-extra": "10.0.0",
    "got": "^11.8.2",
    "js-yaml": "^3.13.1",
    "lodash.defaults": "^4.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.uniqby": "^4.7.0",
