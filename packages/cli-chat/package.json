{
  "name": "@dxos/cli-chat",
  "version": "2.16.2",
  "description": "DXOS Chat CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-chat#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "bin": {
    "dx-chat": "bin/dx-chat.js"
  },
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
