  "dependencies": {
    "@dxos/cli-core": "workspace:*",
    "@dxos/crypto": "~2.28.5",
    "@dxos/debug": "~2.28.5",
    "@dxos/network-manager": "~2.28.5",
    "@dxos/protocol-plugin-messenger": "~2.28.5",
    "@dxos/protocol-plugin-presence": "~2.28.5",
    "assert": "^2.0.0",
    "hypercore-crypto": "^1.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.24",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "uuid": "^3.3.3",
    "@types/node": "~16.11.12",
    "@types/yargs": "^16.0.1",
    "@types/jest": "~27.0.3"
