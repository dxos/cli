    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^25.1.0",
    "prettier": "^1.14.2",
    "semistandard": "14.2.0",
    "uuid": "^3.3.3"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
