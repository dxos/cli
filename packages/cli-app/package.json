{
  "name": "@dxos/cli-app",
  "version": "2.20.0",
  "description": "DXOS App CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-app#readme",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "license": "AGPL-3.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dx-app": "bin/dx-app.js",
    "dx-app-server": "bin/dx-app-server.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc && cp -R ./*.yml dist",
    "build:watch": "tsc --watch",
    "clean": "rm -rf ./dist",
    "cli": "ts-node --files ./src/index.ts",
    "dx": "../cli/bin/dx.js",
    "lint": "eslint './src/**/*.{ts,js}'",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "server": "npm run build && npm run cli app serve start",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "preset": "ts-jest/presets/default",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "workspace:*",
    "@dxos/debug": "~2.32.0",
    "@dxos/registry-client": "~2.32.0",
    "@dxos/util": "^2.32.0",
    "@otplib/plugin-thirty-two": "^12.0.1",
    "@polkadot/api": "4.17.1",
    "assert": "^2.0.0",
    "body-parser": "^1.19.0",
    "boolean": "^3.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "express": "^4.17.1",
    "find-up": "~5.0.0",
    "js-yaml": "^3.13.1",
    "lodash-clean": "^2.2.1",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "node-fetch": "^2.6.1",
    "url-join": "^4.0.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@dxos/cli-dxns": "workspace:*",
    "@dxos/eslint-plugin": "~1.0.27",
    "@types/body-parser": "^1.19.1",
    "@types/cli-progress": "^3.9.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "~4.1.7",
    "@types/express": "^4.17.13",
    "@types/get-folder-size": "^3.0.1",
    "@types/jest": "~27.0.3",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "~16.11.12",
    "@types/pify": "~5.0.1",
    "@types/qrcode": "^1.4.1",
    "@types/url-join": "^4.0.1",
    "@types/yargs": "^17.0.10",
    "eslint": "^7.13.0",
    "jest": "^27.4.4",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
