{
  "name": "@dxos/cli-console",
  "version": "2.4.14-alpha.0",
  "description": "DXOS Console CLI",
  "homepage": "https://github.com/dxos/cli/tree/master/packages/cli-console#readme",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dxos/cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/cli.git"
  },
  "main": "dist/es/index.js",
  "bin": {
    "wire-console": "bin/dx-console.js",
    "dx-console": "bin/dx-console.js"
  },
  "files": [
    "bin/",
    "dist/es"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --copy-files",
    "clean": "rm -rf ./dist",
    "cli": "BABEL_CACHE_PATH=./dist/babel-cache.json babel-node ./src/index.js",
    "lint": "semistandard",
    "lint:fix": "semistandard --fix",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "test": "jest --no-cache --silent --runInBand"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest",
    "verbose": false
  },
  "dependencies": {
    "@dxos/cli-core": "^2.4.14-alpha.0",
    "@dxos/debug": "~2.12.22",
    "@wirelineio/registry-client": "^1.1.0-beta.0",
    "assert": "^2.0.0",
    "chance": "^1.1.3",
    "download": "^7.1.0",
    "fs-extra": "^8.1.0",
    "hypercore-crypto": "^1.0.0",
    "ipfs-http-client": "42.0.0",
    "js-yaml": "^3.13.1",
    "lodash-clean": "^2.2.1",
    "lodash.isequal": "^4.5.0",
    "node-fetch": "^2.6.0",
    "node-yaml": "^4.0.1",
    "semver": "^7.1.1",
    "tar": "^6.0.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.10",
    "eslint": "^7.13.0",
    "jest": "^27.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5",
    "uuid": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  },
  "peerDependencies": {
    "@dxos/cli": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c147f853bc07376a163cee05c3c44198387c5655"
