{
  "name": "@dxos/cli-e2e-test",
  "version": "2.12.1",
  "description": "E2E test script for CLI packages",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test": "exit 0",
    "test:e2e": "mocha 'src/**/*.test.ts'"
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "timeout": 0,
    "bail": true
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.20",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "eslint": "^7.13.0",
    "expect": "^27.2.1",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "mocha": "^9.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "@types/node": "~16.11.12",
    "@types/express": "^4.17.13",
    "@types/yargs": "^16.0.1",
    "@dxos/botkit": "~2.19.11",
    "fs-extra": "10.0.0",
    "@types/fs-extra": "^9.0.13",
    "@dxos/crypto": "~2.19.11",
    "@dxos/async": "~2.19.11",
    "@dxos/cli-core": "workspace:*"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  }
}
