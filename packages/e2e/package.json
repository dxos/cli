{
  "name": "@dxos/cli-e2e-test",
  "version": "2.6.1-alpha.0",
  "description": "E2E test script for CLI packages",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint './src/**/*.{ts,js}'",
    "test:e2e": "mocha 'src/**/*.test.ts'"
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "timeout": 0,
    "bail": true
  },
  "devDependencies": {
    "@dxos/eslint-plugin": "~1.0.10",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "eslint": "^7.13.0",
    "expect": "^27.2.1",
    "express": "^4.17.1",
    "got": "^10.7.0",
    "mocha": "^9.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/recomended"
  }
}
